mixin header()
  <!DOCTYPE html>
  html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible", content="ie=edge")
    link(rel="stylesheet", href="assets/css/index.css")
    title Document
  body
    header
      .wrapper
        .header_phone
          .header_phone-tel +7 423 273-26-80
          a(href="#").header_phone-button_small обратный звонок
        .header_address
          .header_address-street Россия, Владивосток, ул. Проходная 4-я, 14
          .header_address-time режим работы: 09:00 - 18:30
          a(href="#").header_address-mail info@logisticforce.ru
        .header_logo
        .header_buttons
          a(href="#").header_buttons-button_enter Войти в личный кабинет
          a(href="#").header_buttons-button_reg Регистрация для новых клиентов
        .header_langs
          .header_lang.header_lang__active 
            img(src="data:image/png;base64,", alt="")
            .header_lang-text РУС
          .header_lang
            img(src="data:image/png;base64,", alt="")
            .header_lang-text 漢語
            
